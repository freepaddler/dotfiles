_env_is_bash || return

_env_os_is darwin && {
    _env_is_bash && _env_source /Library/Developer/CommandLineTools/usr/share/git-core/git-completion.bash
    _env_is_zsh && _env_source /Library/Developer/CommandLineTools/usr/share/git-core/git-completion.zsh
}

